<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="650.0" prefWidth="930.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.principal.LoginController">
   <children>
      <Label fx:id="isConnected" alignment="CENTER" layoutX="277.0" layoutY="53.0" prefHeight="46.0" prefWidth="314.0" text="Status" textFill="#cd1313">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtNomeUsuario" layoutX="345.0" layoutY="112.0" promptText="Nome do Usuário" />
      <PasswordField fx:id="txtSenhaUsuario" layoutX="345.0" layoutY="153.0" promptText="Senha do usuário" />
      <Button layoutX="345.0" layoutY="195.0" mnemonicParsing="false" onAction="#login" text="Entrar" />
      <Button layoutX="452.0" layoutY="195.0" mnemonicParsing="false" onAction="#limpar" text="Limpar" />
   </children>
</AnchorPane>
